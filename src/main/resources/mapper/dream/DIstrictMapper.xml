<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dreamfield.dreamapi.mapper.dream.DistrictMapper" >
    <select id="getDistrict" resultType="com.dreamfield.dreamapi.response.DistricResponse">
        select id districtId,
              name districtName
        from district
        where parent_id =#{parentId}
    </select>


</mapper>

